<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="gruppo3.ifoasapereutile" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".ParseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".InformaticaActivity"
            android:label="@string/title_activity_informatica" >
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name=".ELearning"
            android:label="@string/title_activity_elearning" >
        </activity>
        <activity
            android:name=".RefreshNavigationDrawerActivity"
            android:label="@string/title_activity_refresh_navigation_drawer" >
        </activity>
        <activity
            android:name=".MieiCorsiActivity"
            android:label="@string/title_activity_miei_corsi" >
        </activity>
        <activity
            android:name=".DettaglioCorsoActivity"
            android:label="@string/title_activity_dettaglio_corsi" >
        </activity>
    </application>

</manifest>
